    <md:SingleSignOnService Location="https://idm.clarin.eu/saml-proxy/saml2idp-web" Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"/>
    <md:SingleSignOnService Location="https://idm.clarin.eu/saml-proxy/saml2idp-web" Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"/>
