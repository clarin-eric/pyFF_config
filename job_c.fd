# Automated job C: publish production SAML metadata about each identity federationâ€™s IdPs.
- load:
    - about_identity_federations_md.xrd
- select:
    - "file://output/id_feds/aconet.xml!//md:EntityDescriptor[md:IDPSSODescriptor and not(@entityID='https://openidp.aco.net/saml')]"
    - "file://output/id_feds/arnesaai_edugain.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/belnet.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/dfn-aai-basic.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/edugain.xml!//md:EntityDescriptor[md:IDPSSODescriptor and md:Extensions/mdrpi:RegistrationInfo/@registrationAuthority='http://ukfederation.org.uk' and not(@entityID='https://idp.protectnetwork.org/protectnetwork-idp' or @entityID='https://idp.painless-security.com/idp/shibboleth' or @entityID='https://idp1.proofidcloud.co.uk/idp/shibboleth' or @entityID='https://sso-demo.proofidcloud.co.uk/idp/pingfederate')]"
    - "file://output/id_feds/eduidcz.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/haka.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/kalmar.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/surfconext.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
    - "file://output/id_feds/swamid_edugain.xml!//md:EntityDescriptor[md:IDPSSODescriptor]"
- xslt:
    stylesheet: tidy.xsl
- finalize:
    name: "http://www.clarin.eu/spf"
    cacheDuration: PT5H
    validUntil: P10D
- publish: output/prod_md_about_spf_idps.xml
- stats
- certreport
